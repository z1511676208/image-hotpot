th: '/element',\n        icon: 'el-icon-message',\n        name: 'Element',\n        children: [{\n          path: '/element/table',\n          name: 'Table'\n        }, {\n          path: '/element/form',\n          name: 'Form'\n        }]\n      }],\n      defaultActive: this.activeTab,\n      // 默认打开的tabs: ['/feature']\n      defaultOpeneds: []\n    };\n  },\n  mounted: function mounted() {\n    var path = this.$route.path;\n\n    if (path.indexOf('/feature/TabsView') === -1) {\n      this.defaultActive = path;\n    } else {\n      this.defaultActive = '/feature/TabsView';\n    }\n  },\n  methods: {\n    routerLinks: function routerLinks(path) {\n      if (this.$route.path !== path) {\n        this.$router.push(path);\n      }\n    },\n    addTabsView: function addTabsView() {\n      console.log('作弊添加TabsView');\n      this.$router.push(\"/feature/TabsView\".concat(new Date() + Math.floor(Math.random() * 100)));\n    }\n  }\n};",{"version":3,"sources":["aside.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgDA,eAAA;AACA,EAAA,IAAA,EAAA,OADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA;;;AAGA,MAAA,WAAA,EAAA,CACA;AACA,QAAA,IAAA,EAAA,GADA;AAEA,QAAA,IAAA,EAAA,WAFA;AAGA,QAAA,IAAA,EAAA,gBAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OADA,EAOA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,IAAA,EAAA,+BAHA;AAIA,QAAA,IAAA,EAAA,SAJA;AAKA,QAAA,QAAA,EAAA,CACA;AACA,UAAA,IAAA,EAAA,yBADA;AAEA,UAAA,IAAA,EAAA;AAFA,SADA,EAKA;AACA,UAAA,IAAA,EAAA,mBADA;AAEA,UAAA,IAAA,EAAA,WAFA;AAGA,UAAA,MAAA,EAAA,KAAA;AAHA,SALA,EAUA;AACA,UAAA,IAAA,EAAA,gBADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAVA;AALA,OAPA,EA4BA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,IAAA,EAAA,UAFA;AAGA,QAAA,IAAA,EAAA,iBAHA;AAIA,QAAA,IAAA,EAAA,SAJA;AAKA,QAAA,QAAA,EAAA,CACA;AACA,UAAA,IAAA,EAAA,gBADA;AAEA,UAAA,IAAA,EAAA;AAFA,SADA,EAKA;AACA,UAAA,IAAA,EAAA,eADA;AAEA,UAAA,IAAA,EAAA;AAFA,SALA;AALA,OA5BA,CAJA;AAiDA,MAAA,aAAA,EAAA,KAAA,SAjDA;AAkDA;AACA,MAAA,cAAA,EAAA;AAnDA,KAAA;AAqDA,GAxDA;AAyDA,EAAA,OAzDA,qBAyDA;AACA,QAAA,IAAA,GAAA,KAA